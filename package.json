    "coveralls": "^3.1.1",
    "cross-fetch": "^3.1.4",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-junit": "^12.2.0",
    "lodash.get": "^4.4.2",
    "prettier": "^2.3.2",
    "webpack": "^5.44.0",
